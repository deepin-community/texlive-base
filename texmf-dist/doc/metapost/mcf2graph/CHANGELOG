[ver. 4.76  / 2022-12-20]
  -change syntax of bond type
   {a<}=dl => {a'}=dr
   {a<}=dr => {a'}=dl
  -change syntax of fused ring
   {(a<,b)}=?6  => (a',b)=?6
   {(a<,b)}=?5  => (a',b)=?5
   {(a<,b<)}=?6 => (a',b')=?6
  -change syntax of serial number'
    {a--b} => {a:b}
  -update mcf_data_base.mcf
  -update MCF manual,example

[ver. 4.75  / 2022-12-12]
  -change syntax of bond type
   {a<}=dl : {a}=dr
   {a<}=dr : {a}=dl
  -change syntax of fused ring
   {(a<,b)}=?6  : (a,b)=?6[3]
   {(a<,b)}=?5  : (a,b)=?5[2]
   {(a<,b<)}=?6 : (a,b)=?6[2]
  -update mcf_data_base.mcf
  -update MCF manual,example

[ver. 4.74  / 2021-11-23]
  -change syntax of fused ring
   (a,b)=?6[4] => (a,b)=?6
   (a,b)=?5[3] => (a,b)=?5
   (a,b)=?4[2] => (a,b)=?4
  -update mcf_data_base.mcf
  -update MCF manual,example

